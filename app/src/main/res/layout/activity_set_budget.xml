                                android:id="@+id/progressResidential"
                                style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:progressDrawable="@drawable/progress_drawable"
                                android:layout_centerInParent="true"
                                android:secondaryProgressTint="#FFF6F6F6"
                                android:progressTint="@color/primary"/>

                            <TextView
                                android:padding="4dp"
                                android:id="@+id/tvResidentialPercentage"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="0%"
                                android:textColor="@color/black"
                                android:fontFamily="@font/poppins_light"
                                android:textSize="10dp"/>
                        </RelativeLayout>
                        <ImageView
                            android:id="@+id/detailResidential"
                            android:layout_gravity="center"
                            android:layout_width="11dp"
                            android:layout_height="18dp"
                            android:src="@drawable/ic_arrow_right"/>
                    </LinearLayout>
                </LinearLayout>
                <com.google.android.material.button.MaterialButton
                    android:layout_gravity="center"
                    android:id="@+id/setBudget"
                    android:layout_width="192dp"
                    android:layout_height="39dp"
                    android:text="Set Budget"
                    android:textAllCaps="false"
                    android:textColor="@color/black"
                    app:cornerRadius="24dp"
                    android:textSize="18dp"
                    android:fontFamily="@font/poppins_medium"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:backgroundTint="@null"
                    android:backgroundTint="@null"
                    android:background="@drawable/bg_button_setbudget"
                    app:rippleColor="#33FFFFFF"
                    app:layout_constraintBottom_toBottomOf="parent"/>
            </LinearLayout>

        </androidx.cardview.widget.CardView>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>